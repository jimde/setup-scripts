[global_config]
  window_state = maximise
[keybindings]
[profiles]
  [[default]]
    allow_bold = False
    background_color = "#1e2229"
    cursor_color = "#aaaaaa"
    foreground_color = "#ffffff"
    show_titlebar = False
    scrollback_infinite = True
[layouts]
  [[default]]
    [[[window0]]]
      type = Window
      parent = ""
    [[[child1]]]
      type = Terminal
      parent = window0
[plugins]
