[push]
	default = simple

[core]
	editor = vim

[url "git@github.com"]
	insteadOf = gh

[url "https://github.com"]
	insteadOf = ghh

[color]
	ui = auto

[alias]
    s = status
    pr = pull --rebase
    c = checkout
    cb = checkout -b
    b = branch
    d = diff
    ds = diff --staged
